		"nodemon": "^2.0.6",
		"sass": "^1.27.0",
		"sass-loader": "^10.0.4",
		"style-loader": "^2.0.0",
		"supertest": "^6.0.1",
		"webpack": "^5.2.0",
		"webpack-bundle-analyzer": "^3.9.0",
