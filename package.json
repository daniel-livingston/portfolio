{
	"name": "daniellivingston.net",
	"version": "1.0.0",
	"description": "The personal showcase website for Daniel Livingston.",
	"main": "index.js",
	"scripts": {
		"start": "node server/index",
		"start:dev": "env-cmd -f ./config/sendgrid.env nodemon server/index",
		"build": "webpack --config=webpack.production.config.js",
		"build:dev": "webpack --config=webpack.development.config.js --watch --progress",
		"build:start": "npm run build && npm start",
		"dev": "concurrently \"npm run build:dev\" \"npm run start:dev\"",
		"test": "jest --config=jest.config.json --runInBand --detectOpenHandles"
	},
	"author": "Daniel Livingston",
	"license": "ISC",
	"dependencies": {
		"@sendgrid/mail": "^7.4.0",
