		"normalize.css": "^8.0.1",
		"prop-types": "^15.7.2",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-router-dom": "^5.2.0",
		"serve-favicon": "^2.5.0",
		"validator": "^13.1.17"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-transform-runtime": "^7.12.1",
